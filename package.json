{
  "name": "deified-machines",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "dup": "jscpd --config .jscpd.json",
    "deadcode": "knip",
    "unimported": "unimported",
    "cycles": "madge --circular --extensions ts,tsx,js,jsx .",
    "depcheck": "depcheck --skip-missing=true",
    "depcruise": "dependency-cruiser --validate dependency-cruiser.cjs .",
    "audit:deps": "npm audit",
    "audit:all": "npm run lint && npm run typecheck && npm run dup && npm run deadcode && npm run unimported && npm run cycles && npm run depcheck && npm run depcruise",
    "clean": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jscpd": "^4.0.5",
    "knip": "^5.63.1",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.2",
    "unimported": "^1.31.0",
    "vite": "^6.2.0"
  }
}
